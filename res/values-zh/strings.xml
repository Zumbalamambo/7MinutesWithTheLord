    <string name="thanksgiving">感谢</string>
    <string name="thanksgiving_desc">为着你在日常生活中的一切人，事，物，感谢祂并赞美祂 &#8211; 弗五:20; 帖前五:18</string>
    
    <string name="petition">祈求</string>
